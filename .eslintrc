{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "nuxt"
  ],
  "extends": [
    "@nuxtjs/eslint-config",
    "plugin:nuxt/recommended",
    "plugin:prettier/recommended"
  ],

  "rules": {
    "vue/multi-word-component-names": "off",
    "vue/no-multiple-template-root": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}
